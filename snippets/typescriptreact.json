{
  "Quick console.log": {
    "prefix": "cons",
    "body": ["console.log('$1', $1)"]
  },
  "React Console component": {
    "prefix": "Cons",
    "body": [
      "import Console from \"@apps/components/Console\";",
      "<Console log={`$1`} />"
    ]
  },
  "Import for aktuell project": {
    "prefix": "imp",
    "body": ["import $1 from \"@apps/$2/components/$1\";"]
  },
  "Zustand store value": {
    "prefix": "zus",
    "body": ["const $1 = useStore(state => state.$1)"]
  },
  "Styled component": {
    "prefix": "stp",
    "body": ["const $1 = styled.${2:View}`", "$3", "`"]
  },
  "Import react": {
    "prefix": "rbase",
    "body": [
      "import React from \"react\"",
      "",
      "export default () => {",
      "$1",
      "return <></>",
      "}"
    ]
  }
}
